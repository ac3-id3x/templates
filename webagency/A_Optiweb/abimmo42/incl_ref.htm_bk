<!-- BOUCLE d'appel des infos agence pour afficher NOM agence + VILLE agence de façon DYNAMIQUE (infos SAISS) DANS LE CAS D'UN MONO AGENCE -->
$$B:IMMOBW:AGENCES:ALIAS:TEXTEDYNAMIQUE$$  
$$BIF:TEXTEDYNAMIQUE$$
$$DO$$ $$LOOP$$
$$BFIN$$
 
<!-- Définition de L'IDPUB de l'agence à afficher sur le site -->  
 $$SET:IDPUB_AGENCE=@@1_TEXTEDYNAMIQUE:IDPUBLICATION@@$$
<!-- Définition du NOM de l'agence à afficher sur le site -->
$$SET:NOM_AGENCE=@@1_TEXTEDYNAMIQUE:NOM@@$$
<!-- Définition de l'adresse à afficher sur le site  -->
$$SET:ADRESSE_AGENCE=@@1_TEXTEDYNAMIQUE:ADRESSE@@$$
<!-- Définition de la ville à afficher sur le site  -->
$$SET:VILLE_AGENCE=@@1_TEXTEDYNAMIQUE:VILLE@@$$
<!-- Définition du CP à afficher sur le site  -->
$$SET:CP_AGENCE=@@1_TEXTEDYNAMIQUE:CP@@$$
<!-- Définition du DEPARTEMENT à afficher sur le site  -->
$$SET:DEP_AGENCE=@@1_TEXTEDYNAMIQUE:DEPARTEMENTAFF@@$$
<!-- Définition du tel à afficher sur le site  -->
$$SET:TEL_AGENCE=@@1_TEXTEDYNAMIQUE:TEL@@$$
<!-- Définition du fax à afficher sur le site  -->
$$SET:FAX_AGENCE=@@1_TEXTEDYNAMIQUE:FAX@@$$
 
 
 

<!-- EXPRESSIONS ALT + TITLE --> 
 

<!-- OBLIGATOIRE definition des expressions pour les alt et title (même si pas d'option de ref !) A modifier selon cahier des charges ou laisser en auto si pas de réf -->
<!-- Si mutli agence les metas par defaut doivent être modifiées -->

$$SET:EXPRESSION1=abicom immobilier$$
$$SET:EXPRESSION2=achat appartement saint etienne$$
$$SET:EXPRESSION3=achat commerce loire 42$$
$$SET:EXPRESSION4=achat maison loire 42$$
$$SET:EXPRESSION5=agence immobiliere saint etienne$$
$$SET:EXPRESSION6=immobilier loire 42$$
$$SET:EXPRESSION7=immobilier st etienne$$
 

<!-- TITRE DERCREF ET KEYWORDS PAR DEFAUT  SI PAGE NON OPTIMISEE--> 
 
$$SET:PAGE:TITR=Agence immobili&egrave;re @@NOM_AGENCE@@ - Immobilier @@VILLE_AGENCE@@ $$
$$SET:PAGE:DESCREF=L'immobilier &agrave; @@VILLE_AGENCE@@ avec l'agence immobili&egrave;re @@NOM_AGENCE@@ &agrave; @@VILLE_AGENCE@@$$
$$SET:PAGE:KEYWORDS=Immobilier @@VILLE_AGENCE@@, agence immobili&egrave;re  @@VILLE_AGENCE@@ @@NOM_AGENCE@@$$
 

<!--  OPTIS REF DE BASE PAGES INDEX AGENCE ET PLAN AVANT OPTIMISATION--> 
 
$$IF=:INDEX:PAGE:IDT$$
$$A:PAGE:TITR=Agence immobili&egrave;re @@NOM_AGENCE@@ - Immobilier @@VILLE_AGENCE@@ $$
$$A:PAGE:DESCREF=L'immobilier &agrave; @@VILLE_AGENCE@@ avec l'agence immobili&egrave;re @@NOM_AGENCE@@  &agrave; @@VILLE_AGENCE@@ : annonces immobili&egrave;res sur @@VILLE_AGENCE@@ et ses environs$$
$$A:PAGE:KEYWORDS=Agence immobili&egrave;re @@NOM_AGENCE@@ @@VILLE_AGENCE@@, immobilier @@VILLE_AGENCE@@$$
$$END$$
 
$$IF=:AGENCEIMMOBILIERE:PAGE:IDT$$
$$A:PAGE:TITR=Agence immobili&egrave;re @@VILLE_AGENCE@@ - @@SRV@@$$
$$A:PAGE:DESCREF=L' agence immobili&egrave;re &agrave; @@VILLE_AGENCE@@, sp&eacute;cialiste de l'immobilier &agrave; @@VILLE_AGENCE@@$$
$$A:PAGE:KEYWORDS=Agence immobili&egrave;re @@VILLE_AGENCE@@$$
$$END$$
 
$$IF=:PLAN:PAGE:IDT$$
$$A:PAGE:TITR=Plan du site : @@SRV@@$$
$$A:PAGE:DESCREF=Plan du site de l'agence @@SRV@@ &agrave; @@VILLE_AGENCE@@$$
$$A:PAGE:KEYWORDS=Immobilier @@VILLE_AGENCE@@$$
$$END$$
 
