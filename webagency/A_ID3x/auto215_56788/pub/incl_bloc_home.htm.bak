<script>
	$(document).ready(function() {
	    var containerCarousel = $('.module-services-items');
	    var args = {
	        margin : 30,
	        navText : ["<i class='mf-icon-arrow-left'></i>", "<i class='mf-icon-arrow-right'></i>"],
	        responsive:{
	            0 : {
	                items: 1,
	                dots : true,
	                nav  : false,
	                navText : ""
	            },
	            670 : {
	                items: 2
	            },
	            1024 : {
	                items  : 3,
	                dots   : false
	            },
	            1280 : {
	                items: $module_items_services,
	                autoplay : $module_autoplay,
	                autoplayTimeout : $module_autoplay_delay,
	                autoplayHoverPause : $module_autoplay_stop,
	                nav: true,
	                dots: false
	            }
	        }
	    };
	    var owlModuleAnnonces = containerCarousel.owlCarousel(args);

	    if ($module_state == 'fixed') {
	        $(window).resize(function() {
	            if ( $(window).width() > 767 ) {
	                stopCarousel();
	            } else {
	                startCarousel();
	            }
	        });
	        if ( $(window).width() > 767 ) {
	            stopCarousel();
	        }

	        function stopCarousel(){
	            owlModuleAnnonces.trigger("destroy.owl.carousel");
	            containerCarousel.removeClass("owl-carousel");
	        }
	        function startCarousel(){
	            containerCarousel.addClass("owl-carousel");
	            containerCarousel.owlCarousel(args);
	        }
	    }
	});
    var $module_items_services = 1;
    $$IF0:ROTATION_MODULE$$
        var $module_state = 'fixed';
    $$END$$
    $$IF=:1:ROTATION_MODULE$$
        var $module_state = 'rotate';
    $$END$$
    $$IF0:SPEED_MODULE$$
        var $module_autoplay = false;
        var $module_autoplay_delay = 0;
        var $module_autoplay_stop = false;
    $$END$$
    $$IF:SPEED_MODULE$$
        var $module_autoplay = true;
        var $module_autoplay_delay = $$SPEED_MODULE$$;
        var $module_autoplay_stop = true;
    $$END$$
</script>