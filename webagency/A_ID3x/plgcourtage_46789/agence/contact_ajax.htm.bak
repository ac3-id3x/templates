        <div class="row-fluid">
            <div class="span8 ctc-coord-agence">
                <span class="ctc-nom-agence">$$CV:NOM ncase$$<br></span>
                
                $$IF!:CPVILLEAGENCE:CV:ADRESSE$$<span itemprop="streetaddress" class="ctc-adresse-agence">$$CV:ADRESSE rxreplace (.*)\s(\d{5})\s(.*) $1 trim$$<br></span>$$END$$
                <span itemprop="postalCode" class="ctc-cp-agence">$$CV:CP$$</span> <span itemprop="addressLocality" class="ctc-ville-agence">$$CV:VILLE$$</span>

            </div>
            $$REM$$<!-- TEL AGENCE -->$$END$$
            <div class="span4">
                <!--#include virtual="agence/incl_telephone_agence.htm" -->
            </div>
        </div>

        $$REM$$<!-- INFOS AGENCE -->$$END$$
        <ul class="margin-top-20 ctc-infos-agence">
            $$REM$$<!-- RESPONSABLE AGENCE -->$$END$$
            $$IF:CV:RESPAG$$
            $$IF!:1:HIDENOMRESPONSABLE$$
