<div class="row-fluid">
    <div class="span2 hidden-phone"></div>
    <div class="span8">
        <form method="post" action="/agence,contact.htm?idp=$$CV:IDPUBLICATION$$" enctype="multipart/form-data" class="ctc-form">
            <!--#include virtual="contact/incl_honeypot.htm" -->
            <input type="hidden" name="u" value="ID3:FORMMAIL" />
            <input type="hidden" name="page_ok" value="/agence,contact_envoi.htm" />
            <input type="hidden" name="page_err" value="/agence,contact.htm" />
            <input type="hidden" name="page_html" value="/agence,contact_email.htm" />
            <input type="hidden" name="cfg_form" value="contact" />
            <input type="hidden" name="cfg_emails" value="contact" />
            <input type="hidden" name="subject" value="Une demande depuis votre site internet !"/>
            <input type="hidden" name="email_to" value="$$CONTACT_EMAIL$$"/>

            <div class="ctc-field ctc-field-input ctc-field-required $$IF:FORM:NOM_FROM$$focused$$END$$">
                <input type="text" class="ctc-field-txt" name="nom_from" value="$$FORM:NOM_FROM$$">
                <label class="ctc-field-label">$$LG:NOM pcase$$ / $$LG:PRENOM pcase$$</label>
                <i class="ctc-field-icon big icon-v5-users"></i>
            </div>

            <div class="row-fluid">
                <div class="span6">
                    <div class="ctc-field ctc-field-input ctc-field-required $$IF:FORM:TEL_FROM$$focused$$END$$">
                        <input type="text" class="ctc-field-txt" name="tel_from" value="$$FORM:TEL_FROM$$">
                        <label class="ctc-field-label">$$LG:TEL pcase$$</label>
                        <i class="ctc-field-icon big icon-v5-phone"></i>
                    </div>
                </div>
                <div class="span6">
                    <div class="ctc-field ctc-field-input ctc-field-required $$IF:FORM:EMAIL_FROM$$focused$$END$$">
                        <input type="text" class="ctc-field-txt" name="email_from" value="$$FORM:EMAIL_FROM$$">
                        <label class="ctc-field-label">$$LG:EMAIL pcase$$</label>
                        <i class="ctc-field-icon big icon-v5-paperplane"></i>
                    </div>
                </div>
            </div>

            <div class="ctc-field ctc-field-input $$IF:FORM:VILLE_FROM$$focused$$END$$">
                <input type="text" class="ctc-field-txt" name="ville_from" value="$$FORM:VILLE_FROM$$">
                <label class="ctc-field-label">$$LG:VILLE pcase$$</label>
                <i class="ctc-field-icon big icon-v5-location"></i>
            </div>

            <div class="ctc-field ctc-field-select ctc-field-required $$IF:FORM:OBJET_FROM$$filled$$END$$">
                <select class="ctc-field-txt" name="objet_from">
                    <option value="">Objet de votre message</option>
                    $$IF:CONTACT_OBJET_1$$
                        <option $$IF=:CONTACT_OBJET_1:FORM:OBJET_FROM$$ selected="selected"$$END$$>$$CONTACT_OBJET_1$$</option>
                    $$END$$
                     $$IF:CONTACT_OBJET_2$$
                        <option $$IF=:CONTACT_OBJET_1:FORM:OBJET_FROM$$ selected="selected"$$END$$>$$CONTACT_OBJET_2$$</option>
                    $$END$$
                     $$IF:CONTACT_OBJET_3$$
                        <option $$IF=:CONTACT_OBJET_1:FORM:OBJET_FROM$$ selected="selected"$$END$$>$$CONTACT_OBJET_3$$</option>
                    $$END$$
                    <option $$IF=:Je souhaite laisser mes critères de recherche de location:FORM:OBJET_FROM$$ selected="selected"$$END$$>Je souhaite laisser mes critères de recherche de location</option>
                    <option $$IF=:Je souhaite laisser mes critères de recherche pour l'achat de mon futur logement:FORM:OBJET_FROM$$ selected="selected"$$END$$>Je souhaite laisser mes critères de recherche pour l'achat de mon futur logement</option>
                    <option $$IF=:Obtenir des renseignements pour louer mon bien:FORM:OBJET_FROM$$ selected="selected"$$END$$>Obtenir des renseignements pour louer mon bien</option>
                    <option $$IF=:Obtenir des renseignements pour vendre mon bien:FORM:OBJET_FROM$$ selected="selected"$$END$$>Obtenir des renseignements pour vendre mon bien</option>
                    <option $$IF=:Je suis intéressé(e) par une annonce de location:FORM:OBJET_FROM$$ selected="selected"$$END$$>Je suis intéressé(e) par une annonce de location</option>
                    <option $$IF=:Je suis intéressé(e) par une annonce de vente:FORM:OBJET_FROM$$ selected="selected"$$END$$>Je suis intéressé(e) par une annonce de vente</option>
                    <option $$IF=:Je souhaite etre rappelé(e):FORM:OBJET_FROM$$ selected="selected"$$END$$>Je souhaite etre rappelé(e)</option>
                    <option $$IF=:Autre:FORM:OBJET_FROM$$ selected="selected"$$END$$>Autre</option>
                </select>
                <label class="ctc-field-label icon-v5-list big"></label>
            </div>

            <div class="ctc-field ctc-field-textarea $$IF:FORM:MESSAGE$$focused$$END$$">
                <textarea class="ctc-field-txt" name="message" rows="7">$$FORM:MESSAGE$$</textarea>
                <label class="ctc-field-label">$$LG:MESSAGE pcase$$</label>
                <i class="ctc-field-icon big icon-v5-pencil"></i>
            </div>

            <div class="row-fluid">
                <div class="span7">
                    <div class="typo-action small italic"><i class="ctc-icon-required margin-right-5"></i> $$LG:CHAMPSOBLIGATOIRES pcase$$</div>
                </div>
                <div class="span5">
                    <input type="submit" class="$$TYPEBOUTON$$ $$TYPEBOUTONACTION$$ btn-large btn-block" value="$$LG:ENVOYER2 pcase$$">
                </div>
            </div>

        </form>
    </div>
</div>


