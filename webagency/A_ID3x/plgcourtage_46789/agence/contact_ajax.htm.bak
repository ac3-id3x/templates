                $$END$$
                $$IF=:1:MULTIRESPAGENCE$$
                $$LG:RESPSAGENCE pcase$$ :
                $$END$$
                $$CV:RESPAG$$
            </li>
            $$END$$
            $$END$$

            $$REM$$<!-- EMAIL AGENCE -->$$END$$
            $$IF!:1:HIDE_EMAIL_AGENCE$$
            $$IF:CV:EMAIL$$
            <li class="ctc-email-agence">$$LG:EMAIL pcase$$ : $$CV:EMAIL$$</li>
            $$END$$
            $$END$$
            
            $$REM$$<!-- LIEN PAGE AGENCE -->$$END$$
            <li class="ctc-lien-agence margin-top-10"><p><a href="$$AGENCE_URLVIRT$$" class="$$TYPEBOUTON$$ $$TYPEBOUTONACTION$$ btn">Découvrir notre groupe</a></p></li>
        </ul>

        $$REM$$<!-- SOCIAL AGENCE -->$$END$$
        <div class="margin-top-20 social-ico ctc-social-agence">
            $$IF:URL_FACEBOOK$$
            <a href="$$URL_FACEBOOK$$" title="$$URL_FACEBOOK$$" target="_blank" class="margin-right-5"><span class="icon-v5-facebook-2 xxbiggest$$IF=:1:COULEUR_SOCIAL$$ social-couleur-facebook$$END$$"></span></a>
            $$END$$
            $$IF:URL_TWITTER$$
            <a href="$$URL_TWITTER$$" title="$$URL_TWITTER$$" target="_blank" class="margin-right-5"><span class="icon-v5-twitter-2 xxbiggest$$IF=:1:COULEUR_SOCIAL$$ social-couleur-twitter$$END$$"></span></a>
            $$END$$
            $$IF:URL_GOOGLE_PLUS$$
            <a href="$$URL_GOOGLE_PLUS$$" title="$$URL_GOOGLE_PLUS$$" target="_blank" class="margin-right-5"><span class="icon-v5-googleplus-2 xxbiggest$$IF=:1:COULEUR_SOCIAL$$ social-couleur-googleplus$$END$$"></span></a>
            $$END$$
            $$IF:URL_PINTEREST$$
            <a href="$$URL_PINTEREST$$" title="$$URL_PINTEREST$$" target="_blank" class="margin-right-5"><span class="icon-v5-pinterest xxbiggest$$IF=:1:COULEUR_SOCIAL$$ social-couleur-pinterest$$END$$"></span></a>
            $$END$$
            $$IF:URL_TUMBLR$$
            <a href="$$URL_TUMBLR$$" title="$$URL_TUMBLR$$" target="_blank" class="margin-right-5"><span class="icon-v5-tumblr-2 xxbiggest$$IF=:1:COULEUR_SOCIAL$$ social-couleur-tumblr$$END$$"></span></a>
            $$END$$
            $$IF:URL_YOUTUBE$$
            <a href="$$URL_YOUTUBE$$" title="$$URL_YOUTUBE$$" target="_blank" class="margin-right-5"><span class="icon-v5-youtube-2 xxbiggest$$IF=:1:COULEUR_SOCIAL$$ social-couleur-youtube$$END$$"></span></a>
            $$END$$
            $$IF:URL_DAILYMOTION$$
