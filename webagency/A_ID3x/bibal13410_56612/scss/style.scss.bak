    //---------------------------------------------------------------------------------------------------------
    //      MENU & NAV
    //---------------------------------------------------------------------------------------------------------

.ligne-social-langue .container {
    max-width: 100%;
    width: 100%;
    padding: 0 20px;
}

	
.logo {
	margin-top: -52px;
	@media only screen and (max-width : 767px) {
	margin-top: 0px;
	}
}


.menu-principal >li {
    background:none; 
    border: none;   
    padding: 0 40px;  
    @media only screen and (max-width : 768px) {
        padding: 0 10px;  
    } 
    &:hover {
        background:none;
    }
    >a {            
        &:after {
            display: block;
            content: "";
            width: 0%;
            height: 2px;
            background-color: $color-primary;
            margin-top: -15px;
            -webkit-transition: all .7s ease;
                    transition: all .7s ease;
         }
         &:hover {
            color: $color-important;               
            &:after { 
                background-color: $color-important; 
                width: 25%;
            }
         }
    }
}

.is-active:after {
    display: block;
    content: "";
    height: 2px;
    background-color: $color-important; 
    width: 25%;
}

