@import "color";
// BG SOLO
$bg-uni:yes;

// PICTURE BG OU DEGRADE CSS BG COLOR PRIMARY vs SECONDARY vs BLOCK > SI NO ALL = BG BODY UNI
$picture-bg:yes;
$picture-bg-url:'../images/dark_wall.png';
$picture-bg-repeat-horizontal:no;

// SIZE SCROLLING
$height-scrolling:900;

// SIZE VIDEO
$height-video:650;

// SIZE NAVIGATION
$height-navigation:40;

$margin-top-under-nav-fixe: 185;

// VAR SITE TYPO
$font-body:"Source Sans Pro",verdana,sans-serif;
