.vide{clear:left;}

/* ============================== Diagnostic de performance énergétique ============================== */
#Dpe{width: 100%;}
#Dpe h6{color: $$TXT_TITRE_RUBRIQUE$$; padding: 5px 0;}
/* affichage diagramme consommation energetiques */
#Dpe td#ConsoEnergie{padding: 0 1% 0 0; width: 50%;}
/* affichage diagramme émissions de gaz à effet de serre */
#Dpe td#EmissionGES{padding: 0 0 0 1%; width: 48%;}
 #Dpe td#EmissionGES #DiagGES{background: url(/z/webagency/slagence_X_V2/detail/img_general/diag_ges_hab.png) 15px 13px no-repeat #fff;}
 #Dpe td#EmissionGES #DiagGESPro{background: url(/z/webagency/slagence_X_V2/detail/img_general/diag_ges_loccom.png) 15px 13px no-repeat #fff;}
/* styles communs aux 2 diagrammes */
#Dpe .diagramme{position: relative; background: #fff; border: 1px solid #eee;}
/* conteneur lettre bilan Conso */
#Dpe .diagramme .diagLettre{position: absolute; color:#fff; font-size: 16px; left: 210px; width:45px; height:20px; text-align: center; background: url(/z/webagency/slagence_X_V2/detail/img_general/diag-fleche.gif) 0 0 no-repeat;}
/* positionnement lettre bilan Conso */
#Dpe .diagramme .diagA{top:25px}
#Dpe .diagramme .diagB{top:48px}
#Dpe .diagramme .diagC{top:70px}
#Dpe .diagramme .diagD{top:94px}
#Dpe .diagramme .diagE{top:117px}
#Dpe .diagramme .diagF{top:140px}
#Dpe .diagramme .diagG{top:163px}
#Dpe .diagramme .diagH{top:192px}
#Dpe .diagramme .diagI{top:214px}
/* textes bas diagramme logements */
#Dpe .diagramme .bilanConso{padding: 205px 15px 0 15px;}
#Dpe .diagramme .bilanConso .conso{font-weight: bold; font-size: 16px; color: #000;}
#Dpe .diagramme .bilanConso .notes{font-size:10px; text-align: justify; color: #999; padding: 0 0 10px 0;}
/* textes bas diagramme locaux commerciaux */
#Dpe .diagramme .bilanConsoPro{padding: 255px 15px 0 15px;}
#Dpe .diagramme .bilanConsoPro .conso{font-weight: bold; font-size: 16px; color: #000;}
#Dpe .diagramme .bilanConsoPro .notes{font-size:10px; text-align: justify; color: #999; padding: 0 0 10px 0;}

 
