@charset "utf-8";
/* CSS Document */

#header {
		/*border-bottom:1px solid #000;*/
		float:left;
		width:100%;
		/*heught:50px;*/
		font-size:93%;
		height:34px;
		line-height:normal;
		background-color:transparent;
		/*border-bottom:2px solid #3D4246;*/
		position:relative;
		padding:0;
		font-weight:bold;
		}

#header a {
		display:block;
		padding:5px 15px;
		text-decoration:none;
		}

#header a:hover {
		text-decoration:none;
		}

#header a:visited {
		text-decoration:none;
		}

#header ul {
		margin:0;
		padding:10px 0 0 0;
		list-style:none;
		position:absolute;
		}

#header li {
		float:left;
		margin:0;
		padding:0;
		}
		
a.ongleton{color:#FF0606;}
a.ongleton:hover{color:#3D4246;}
a.ongletoff{color:#FFF;}
a.ongletoff:hover{color:#ADB2B6;}
.ongleton{background:url($$-$$img/norm_left_on.gif) no-repeat left top; }
.ongletoff{background:url($$-$$img/norm_left2.gif) no-repeat left top; }
.ongletlion{background:url($$-$$img/norm_right_on.gif) no-repeat right top;border-bottom:2px solid #FFF;}
.ongletlioff{background:url($$-$$img/norm_right2.gif) no-repeat right top;}


