<script language="javascript" type="text/javascript">
	function miniAffiche(str){
		if(str=='link-list'){
			document.getElementById('map-engine').style.display = "block";
			document.getElementById('list-engine').style.display = "none";
			document.getElementById('list-menu').style.backgroundPosition = "0 0";
			document.getElementById('map-menu').style.backgroundPosition = "0 -35px";
			
		}else if(str=='link-map'){
			document.getElementById('map-engine').style.display = "none";
			document.getElementById('list-engine').style.display = "block";
			document.getElementById('list-menu').style.backgroundPosition = "0 -35px";
			document.getElementById('map-menu').style.backgroundPosition = "0 0";
		}
	}	
</script>
