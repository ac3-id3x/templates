$$INCL:COMMON.INC$$<?xml version="1.0" encoding="UTF-8"?>

$$B:IMMOBW:CONFIG-CARROUSELS:ALIAS:CONFIG$$
$$B:IMMOBW:ANN:ALIAS:SEARCH:QRY:IDTT:QRY:LANG:::VAL:CONFIG_QUERY:~worldwide:QRY:WORLDWIDE:~getDtCreationMax:QRY:GETDTCREATIONMAX$$
$$B:IMMOBW:AGENCES::::::,NBANN:ALIAS:AGENCES$$

<homepage>
	<conf>
		<socialsharing>$$PAGE:WAG_SOCIAL_SHARING htmlattributeencode$$</socialsharing>
		<mode>$$CONFIG:MODE$$</mode>
		<nbcarrousels>$$CONFIG:NBCARROUSELS$$</nbcarrousels>
		<nbagences>$$AGENCES:NBR$$</nbagences>
		$$IF:CONFIG:IDPUBLICATION$$<idpublication>$$CONFIG:IDPUBLICATION$$</idpublication>$$END$$
		<galerie>
			<libelle>$$CONFIG:LIBELLEGALERIE$$</libelle>
			<query>$$CONFIG:QUERYGALERIE htmlattributeencode$$</query>
		</galerie>
		$$BIF:CONFIG$$
		$$DO$$
		<carrousel>
			<id>$$CONFIG:ID$$</id>
			<numeroordre>$$CONFIG:NUMEROORDRE$$</numeroordre>
			<libelle>$$CONFIG:LIBELLE$$</libelle>
			<nblignes>$$CONFIG:NBLIGNES$$</nblignes>
			<query>$$CONFIG:QUERY htmlattributeencode$$</query>
			<nbannonces>$$CONFIG:NBANNONCES$$</nbannonces>
		</carrousel>
		$$LOOP$$
		$$BFIN$$
	</conf>

	$$IF=:2:CONFIG:MODE$$
	<carrousel>
		<id>$$CONFIG:CARROUSELID$$</id>
		$$BIF:SEARCH$$
		<listeannonces>
			<nbTrouvees>$$SEARCH:NBRESULT$$</nbTrouvees>
			<nbAffichables>$$SEARCH:NBR$$</nbAffichables>
			$$IF:SEARCH:PAGE$$<pageCourante>$$SEARCH:PAGE$$</pageCourante>$$END$$
			$$IF:SEARCH:NBRPAGES$$<pageMax>$$SEARCH:NBRPAGES$$</pageMax>$$END$$
			$$IF:SEARCH:LIENARRIERE$$<pagePrecedente>http://$$REP:RSRV$$/$$SEARCH:LIENARRIERE htmlattributeencode$$</pagePrecedente>$$END$$
			$$IF:SEARCH:LIENAVANT$$<pageSuivante>http://$$REP:RSRV$$/$$SEARCH:LIENAVANT htmlattributeencode$$</pageSuivante>$$END$$
			$$IF:SEARCH:DTCREATIONMAX$$<dtCreationMax>$$SEARCH:DTCREATIONMAX date yyyy-MM-ddTHH:mm:ss$$</dtCreationMax>$$END$$
			<annonces>
			$$DO:1:50$$
				<!--#include virtual="search_annonce.inc" -->
			$$LOOP$$
			</annonces>
		</listeannonces>
		$$BFIN$$
		</carrousel>
	$$END$$

	$$IF=:1:CONFIG:MODE$$
	<galerie>
		$$BIF:SEARCH$$
		<listeannonces>
			<nbTrouvees>$$SEARCH:NBRESULT$$</nbTrouvees>
			<nbAffichables>$$SEARCH:NBR$$</nbAffichables>
			$$IF:SEARCH:PAGE$$<pageCourante>$$SEARCH:PAGE$$</pageCourante>$$END$$
			$$IF:SEARCH:NBRPAGES$$<pageMax>$$SEARCH:NBRPAGES$$</pageMax>$$END$$
			$$IF:SEARCH:LIENARRIERE$$<pagePrecedente>http://$$REP:RSRV$$/$$SEARCH:LIENARRIERE htmlattributeencode$$</pagePrecedente>$$END$$
			$$IF:SEARCH:LIENAVANT$$<pageSuivante>http://$$REP:RSRV$$/$$SEARCH:LIENAVANT htmlattributeencode$$</pageSuivante>$$END$$
			$$IF:SEARCH:DTCREATIONMAX$$<dtCreationMax>$$SEARCH:DTCREATIONMAX date yyyy-MM-ddTHH:mm:ss$$</dtCreationMax>$$END$$
			<annonces>
			$$DO:1:50$$
				<!--#include virtual="search_annonce.inc" -->
			$$LOOP$$
			</annonces>
		</listeannonces>
		$$BFIN$$
	</galerie>
	$$END$$

	$$BIF:AGENCES$$
		$$DO$$
		$$LOOP$$
	$$BFIN$$
</homepage>
