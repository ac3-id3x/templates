    <script type="text/javascript">    	
        function onSilverlightError(sender, args) {
            var appSource = "";
            if (sender != null && sender != 0) {
                appSource = sender.getHost().Source;
            }

            var errorType = args.ErrorType;
            var iErrorCode = args.ErrorCode;

            if (errorType == "ImageError" || errorType == "MediaError") {
                return;
            }

            var errMsg = "Unhandled Error in Silverlight Application " + appSource + "\n";

            errMsg += "Code: " + iErrorCode + "    \n";
            errMsg += "Category: " + errorType + "       \n";
            errMsg += "Message: " + args.ErrorMessage + "     \n";

            if (errorType == "ParserError") {
                errMsg += "File: " + args.xamlFile + "     \n";
                errMsg += "Line: " + args.lineNumber + "     \n";
                errMsg += "Position: " + args.charPosition + "     \n";
            }
            else if (errorType == "RuntimeError") {
                if (args.lineNumber != 0) {
                    errMsg += "Line: " + args.lineNumber + "     \n";
                    errMsg += "Position: " + args.charPosition + "     \n";
                }
                errMsg += "MethodName: " + args.methodName + "     \n";
            }

            throw new Error(errMsg);
        }

        var paymentStatusString = "";
        var paymentDetailedResponse = "";
        var success = false;
        var statusSet = false;

        function ReportPaymentFailure(valeur) {
            paymentStatusString = valeur;
            success = false;
            statusSet = true;
        }

        function ReportPaymentSuccess(valeur) {
            paymentStatusString = valeur;
            success = true;
            statusSet = true;
        }

        function ReportPaymentResponse(valeur) {
            if (valeur == "success") {
                ReportPaymentSuccess(valeur);
            } else {
                ReportPaymentFailure(valeur);
            }
        }

        function ReportDetailedResponse(valeur) {
            paymentDetailedResponse = valeur;
        }

        function DetailedResponse() {
            return paymentDetailedResponse;
        }

        function PaymentResponse() {
            return paymentStatusString;
        }

        function GetPaymentSuccess() {
            return success;
        }

        function GetStatusSet() {
            return statusSet;
        }

        function SetSlProApplicationVersion(versionNumber) {
            var element = window.document.getElementById("hdnSlproVersionNumber");
            if (element) {
                element.innerHTML = versionNumber;
            }
        }
        
        function RefreshParent()
				{				
    			var plugin = document.getElementById("silverlightControl");
    			var paymentManager = plugin.content.PaymentUIManagerJS;
    			var paymentManagerResult = paymentManager.Refresh();
    			if (paymentManagerResult) {
        		event.returnValue = paymentManagerResult;
    			}
				}
    </script>
