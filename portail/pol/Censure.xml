<Censures>
  <Censure>
    <Titre>Censure commentaires</Titre>
    <Erreur>Le commentaire de l'annonce est trop court.</Erreur>
    <Conditions>
      <And>
        <Simple>
          <Champ>LEN(Commentaires)</Champ>
          <Type>NUMBER</Type>
          <Operateur>&lt;</Operateur>
          <Valeur>15</Valeur>
        </Simple>
        <Simple>
          <Champ>NB de pièces</Champ>
          <Type>NUMBER</Type>
          <Operateur>&gt;</Operateur>
          <Valeur>4</Valeur>
        </Simple>
        <Simple>
          <Champ>Type de bien</Champ>
          <Type>STRING</Type>
          <Operateur>INLIST</Operateur>
          <Valeur>Appartement,Maison</Valeur>
        </Simple>
      </And>  
    </Conditions>
  </Censure>
</Censures>